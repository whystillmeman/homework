    "daytime = df[\"time_of_day\"].values\n",
    "triptime = df[\"trip_duration\"].values\n",
    "\n",
    "\n",
    "evenings = 0\n",
    "evening_count = 0\n",
    "afternoons = 0\n",
    "afternoon_count = 0\n",
    "rush_hour_mornings = 0\n",
    "rush_hour_morning_count = 0\n",
    "rush_hour_evenings = 0\n",
    "rush_hour_morning_count = 0\n",
    "late_nights = 0\n",
    "late_night_count = 0\n",
    "\n",
    "j = 0\n",
    "    \n",
    "for i in daytime:\n",
    "    if daytime.all == 'rush_hour_morning':\n",
    "        rush_hour_mornings += triptime[j]\n",
    "        rush_hour_morning_count = rush_hour_morning_count +1\n",
    "        j = j + 1\n",
    "    elif daytime.all == \"afternoon\":\n",
    "        afternoons += triptime[j]\n",
    "        afternoon_count = afternoon_count + 1\n",
    "        j = j + 1\n",
    "    elif daytime.all == \"rush_hour_evening\":\n",
    "        rush_hour_evenings+= triptime[j]\n",
    "        rush_hour_evening_count =  rush_hour_evening_count + 1\n",
    "        j = j + 1\n",
    "    elif daytime.all == \"evening\":\n",
