   "cell_type": "code",
   "execution_count": 43,
   "metadata": {},
   "outputs": [],
   "source": [
    "pickupcoordinate = df[['pickup_latitude', 'pickup_longitude']].values\n",
    "\n",
    "dropoffcoordinate = df[['dropoff_latitude', 'dropoff_longitude']].values\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 44,
   "metadata": {},
   "outputs": [],
   "source": [
    "import reverse_geocoder as rg\n",
    "\n",
    "pickup_places = []\n",
    "for i in pickupcoordinate:\n",
    "    pickup_places.append(tuple(i))\n",
    "    \n",
    "dropoff_places = []\n",
    "for i in dropoffcoordinate:\n",
    "    dropoff_places.append(tuple(i))\n",
    "\n",
    "   "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 45,
   "metadata": {},
   "outputs": [],
   "source": [
    "geo_pickup = rg.search(pickup_places)\n",
    "geo_dropoff = rg.search(dropoff_places)\n",
    "\n",
    "\n",
    "place_to_pickup = []\n",
    "place_to_dropoff = []\n",
    "\n",
    "for a in geo_pickup:\n",
    "    place_to_pickup.append(a['name'])\n",
    "    \n",
    "for a in geo_dropoff:\n",
    "    place_to_dropoff.append(a['name'])\n",
    "    "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 46,
   "metadata": {},
   "outputs": [],
   "source": [
    "df[\"pickup_district\"] = place_to_pickup\n",
    "\n",
    "df[\"dropoff_district\"] = place_to_dropoff"
   ]
