    "        evening_count = evening_count + 1\n",
    "        j = j + 1\n",
    "    elif daytime.all == \"late_night\":\n",
    "        late_nights += triptime[j]\n",
    "        late_night_count = late_night_count + 1\n",
    "        j = j + 1\n",
    "        \n",
    "rush_morn_ave =  rush_hour_mornings / rush_hour_morning_count\n",
    "after_ave = afternoons /  afternoon_count\n",
    "rush_eve_ave = rush_hour_evenings /  rush_hour_evening_count\n",
    "eve_ave = evenings / evening_count\n",
    "late_night_ave = late_nights / late_night_count\n",
    "\n",
    "labels = 'rush_hour_morning', 'afternoon', 'rush_hour_evening', 'evening' , 'late_night'\n",
    "sizes = [rush_morn_ave, after_ave, rush_eve_ave, eve_ave,late_night_ave ]\n",
    "explode = (0, 0, 0, 0)  # only \"explode\" the 2nd slice\n",
