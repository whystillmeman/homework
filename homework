  {
   "cell_type": "code",
   "execution_count": 62,
   "metadata": {},
   "outputs": [],
   "source": [
    "def timeofday(date):\n",
    "    \n",
    "        \n",
    "    if 9 <= int(date[11:13]) < 16:\n",
    "        return \"afternoon\"\n",
    "    \n",
    "    elif 16 <= int(date[11:13]) < 18:\n",
    "        return \"rush_hour_evening\"\n",
    "\n",
    "    elif 18 <= int(date[11:13]) < 23:\n",
    "        return \"evening\"\n",
    "\n",
    "    elif 23 <= int(date[11:13]) < 24:\n",
    "        return \"late_night\"\n",
    "\n",
    "    elif 0 <= int(date[11:13]) < 7:\n",
    "        return \"late_night\"\n",
    "\n",
    "    elif 7 <= int(date[11:13]) < 9:\n",
    "        return \"rush_hour_morning\"\n",
    "                \n",
    "        \n",
    "   \n",
    "\n",
    "                \n",
    "df[\"time_of_day\"] = df[\"pickup_datetime\"].apply(timeofday)\n",
    "             \n",
    "    \n"
   ]
  },
